<mods:mods 
  xmlns:mods="http://www.loc.gov/mods/v3" 
  xmlns:etd="http://www.ndltd.org/standards/metadata/etdms/1.0/"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema">
  <mods:titleInfo>
    <mods:title/>
  </mods:titleInfo>
  <mods:name ID="" type="personal">
    <mods:namePart type="given"/>
    <mods:namePart type="family"/>
    <mods:displayForm/>
    <mods:affiliation/>
    <mods:role>
      <mods:roleTerm authority="marcrelator" type="text">author</mods:roleTerm>
    </mods:role>
  </mods:name>
  <mods:name ID="" type="personal">
    <mods:namePart type="given"/>
    <mods:namePart type="family"/>
    <mods:displayForm/>
    <mods:role>
      <mods:roleTerm authority="marcrelator" type="text">Thesis Advisor</mods:roleTerm>
    </mods:role>
  </mods:name>
  <mods:name ID="" type="personal">
    <mods:namePart type="given"/>
    <mods:namePart type="family"/>
    <mods:displayForm/> 
    <mods:role>
      <mods:roleTerm type="text">Committee Member</mods:roleTerm>
    </mods:role>
    <mods:description>Emory Committee Member</mods:description>
   </mods:name>
  <mods:name type="personal">
    <mods:namePart type="given"/>
    <mods:namePart type="family"/>
    <mods:displayForm/>
    <mods:affiliation/>
    <mods:role>
      <mods:roleTerm type="text">Committee Member</mods:roleTerm>
    </mods:role>
    <mods:description>Non-Emory Committee Member</mods:description>
  </mods:name>
  <mods:name type="corporate">
    <mods:namePart>Emory University</mods:namePart>
    <mods:role>
      <mods:roleTerm authority="marcrelator" type="text">Degree grantor</mods:roleTerm>
    </mods:role>
  </mods:name>
  <mods:typeOfResource>text</mods:typeOfResource>
  <mods:genre authority="aat"/>
  <mods:genre authority="marc">thesis</mods:genre>
  <mods:originInfo>
    <mods:issuance>monographic</mods:issuance>
    <mods:dateIssued keyDate="yes"/>
  <!-- convert to 4-digit year, set encoding="marc" before handing off to marc transform -->
    <mods:dateOther type="embargoedUntil"/> 
  </mods:originInfo>
  <mods:language>
    <mods:languageTerm authority="iso639-2b" type="text">English</mods:languageTerm>
<!-- FIXME: use appropriate language code here (in record or add before converting to MARC?) -->
    <mods:languageTerm authority="iso639-2b" type="code">eng</mods:languageTerm>
  </mods:language>
  <mods:physicalDescription>
    <mods:form authority="marcform">electronic</mods:form>
    <mods:internetMediaType>application/pdf</mods:internetMediaType>
    <mods:digitalOrigin>born digital</mods:digitalOrigin>
    <mods:note type="physical details">digital, PDF file</mods:note>
    <mods:form authority="gmd">electronic resource</mods:form>
    <mods:extent/> <!-- ### p. -->
  </mods:physicalDescription>
  <mods:abstract/>
  <mods:tableOfContents/>
  <mods:subject authority="keyword">
    <mods:topic/>
  </mods:subject>
  <mods:identifier type="ark"/>
  <mods:identifier type="uri"/>
  <mods:location>
    <mods:url usage="primary display"/>
  </mods:location>
  <mods:accessCondition type="useAndReproduction"/>
  <mods:accessCondition type="restrictionOnAccess">Embargoed for </mods:accessCondition>

  <mods:relatedItem type="host">
    <mods:titleInfo>
     <mods:title>Emory University - Electronic Theses and Dissertations</mods:title>
   </mods:titleInfo>
   <mods:location>
     <mods:url>http://etd.library.emory.edu</mods:url>
   </mods:location>
  </mods:relatedItem>

  <mods:recordInfo>
    <mods:recordIdentifier/>
    <mods:recordCreationDate/>
    <mods:recordChangeDate/>
  </mods:recordInfo>

  <mods:extension>
    <etd:degree>
      <etd:name/>
      <etd:level/>
      <etd:discipline/>
    </etd:degree>
  </mods:extension>

  <mods:note type="partneragencytype" ID="" displayLabel="Type of partner agency"></mods:note>
  <mods:note type="admin" ID="copyright">registering copyright? </mods:note>
  <mods:note type="admin" ID="embargo">embargo requested? </mods:note>
  <mods:note type="admin" ID="pq_submit"></mods:note>
</mods:mods>
