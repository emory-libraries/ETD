<fedora>
  <development>
     <server></server>
     <port></port>
<!-- default account should be a non-privileged user -->
     <username>guest</username>
     <password></password>
<!-- account for automated processes that need more access (should be etdadmin) -->
     <maintenance_account>
       <username></username>
       <password></password>   
     </maintenance_account>
     <protocol>https</protocol>
     <risearch>
	    <requiresAuth>0</requiresAuth>
        <!-- note: port/protocol only required if different than fedora settings -->
        <port></port>
        <protocol></protocol>
      </risearch>
  </development>
  <test extends="development">
     <port></port>
     <username></username>
     <password></password>   
  </test>
  <production extends="development">
     <server></server>
     <port></port>
     <username></username>
     <password></password>
     <maintenance_account>
       <username></username>
       <password></password>   
     </maintenance_account>
     <protocol>https</protocol>     
  </production>
</fedora>
